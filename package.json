{
  "type": "module",
  "name": "node-file-mapping",
  "description": "A package to create, open, read and write memory mapped files.",
  "author": "Fynn Haupt",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/FynniX/node-file-mapping#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FynniX/node-file-mapping.git"
  },
  "bugs": {
    "url": "https://github.com/FynniX/node-file-mapping/issues"
  },
  "keywords": [
    "nodejs",
    "node",
    "typescript",
    "mapping",
    "memory",
    "file"
  ],
  "main": "dist/_virtual/_virtual_multi-entry.cjs",
  "module": "dist/_virtual/_virtual_multi-entry.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/_virtual/_virtual_multi-entry.mjs",
      "require": "./dist/_virtual/_virtual_multi-entry.cjs"
    }
  },
  "scripts": {
    "build": "pnpm run addon:build && rimraf dist && rollup -c",
    "addon:build": "node-gyp rebuild",
    "addon:clean": "node-gyp clean",
    "lint": "eslint lib",
    "lint:fix": "eslint lib --fix",
    "format": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write --ignore-unknown"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.9",
    "eslint": "9.x",
    "globals": "^15.8.0",
    "node-gyp": "^10.1.0",
    "prettier": "3.3.2",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "dependencies": {
    "node-addon-api": "^8.0.0"
  }
}
